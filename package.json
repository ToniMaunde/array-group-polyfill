{
  "name": "array-group-polyfill",
  "version": "0.1.2",
  "description": "A polyfill for the yet to be supported by all browsers Array.prototype.group()",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "release": "npm run build && changeset publish",
    "lint": "tsc"
  },
  "keywords": [
    "arrays",
    "ts",
    "group"
  ],
  "author": "Milton David Alberto",
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "tsup": "^6.7.0",
    "typescript": "^5.1.3"
  },
  "license": "MIT"
}
